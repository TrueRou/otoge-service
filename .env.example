# Server configuration
USAGI_MAIMAI_PROBER_BIND_HOST=127.0.0.1
USAGI_MAIMAI_PROBER_BIND_PORT=7100

# Database configuration
# For SQLite (default): sqlite+aiosqlite:///database.db
# For PostgreSQL: postgresql+asyncpg://user:password@localhost/dbname
# For MySQL: mysql+aiomysql://user:password@localhost/dbname
USAGI_MAIMAI_PROBER_DATABASE_URL=sqlite+aiosqlite:///database.db

# Developer tokens
USAGI_MAIMAI_PROBER_LXNS_DEVELOPER_TOKEN=
USAGI_MAIMAI_PROBER_DIVINGFISH_DEVELOPER_TOKEN=

# Arcade proxy
USAGI_MAIMAI_PROBER_ARCADE_PROXY=